#include <iostream>

using namespace std;
class complex {
    int real, imag;
public:

   complex()
   {
       real=0;
       imag=0;
   }
   complex(int r, int i )
   {
       real=r;
       imag=i;
   }
    complex operator+(complex &c) 
    {
        complex res;
        res.real = real + c.real;
        res.imag = imag + c.imag;
        return res;
    }

    void display() {
        cout << real << "+" << imag << "i" << "\n";
    }
};

int main()
{
   complex c1(1,2),c2(2,3),c3;
   c3=c2+c1;
   c3.display();
    return 0;
}
